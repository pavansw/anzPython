import random
raw = '0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz~!@#$%^&*()_+=-`[]\|:;<.,>/?'
random.sample(raw,8)
random.sample(raw,9)
random.sample(raw,7)
x = random.sample(raw,7)
x
ans = ''
for var in x:
	ans=ans+var
ans
